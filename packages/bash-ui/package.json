{
  "name": "@allowance/bash-ui",
  "version": "0.0.0",
  "main": "./src/index.tsx",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "prepare": "panda codegen --clean",
    "build": "tsup src/index.tsx --external @allowance/styled-system",
    "dev": "tsup src/index.tsx --watch --external @allowance/styled-system",
    "lint": "eslint \"src/**/*.ts*\"",
    "clean": "rm -rf .turbo && find ./node_modules -mindepth 1 -maxdepth 1 ! -regex '^./node_modules/\\@allowance$' | xargs rm -rf && rm -rf dist"
  },
  "devDependencies": {
    "@pandacss/dev": "^0.5.0",
    "@pandacss/types": "^0.5.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "eslint-config-custom": "workspace:*",
    "tsconfig": "workspace:*",
    "tsup": "^5.10.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-navigation-menu": "^1.1.3",
    "@types/prop-types": "^15.7.5",
    "prop-types": "^15.8.1",
    "react": "^18.2.0"
  },
  "publishConfig": {
    "access": "public"
  }
}